{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"]
		},
		"start": {
			"dependsOn": ["^start"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"dev:frontend": {
			"cache": false,
			"persistent": true
		},
		"dev:backend": {
			"cache": false,
			"persistent": true
		}
	}
}
